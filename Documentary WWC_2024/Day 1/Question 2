// Write a program to check wether the number is prime or not
#include <iostream>
using namespace std;
int main() {
    // Write C++ code here
    int n,ans;
    ans=1;
    cout<<"Enter the number";
    cin>>n;
    for (int i=2;i<=n/2;i++){
        if(n%i==0){
            ans=0;
            break;
        }
    }
    if(ans==0){
        cout<<"Non Prime";
    }
    else{
        cout<<"Prime";
    }
    return 0;
}
